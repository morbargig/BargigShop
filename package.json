{
  "name": "bargig-shop",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "node server.js",
    "front": "react-scripts start",
    "front:build": "react-scripts build",
    "server": "node server.js",
    "push": "git add . && git rm -r build && git commit -m \"`echo \"Auto commit by $(git config user.name) at $(date +%Y-%m-%dT%H:%M%z)\"`\"&& git push origin master",
    "heroku": "heroku local -p 8000",
    "herokuPush": "npm run front:build && git add .&&git commit -m \"automatic commit\"&&git push -f heroku master && rm -rf ./build",
    "all": "npm-run-all --parallel server start",
    "build": "",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.15.0",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "@types/firebase": "^3.2.1",
    "@types/jest": "^26.0.24",
    "@types/node": "^12.20.37",
    "@types/react": "^17.0.34",
    "@types/react-dom": "^17.0.11",
    "@types/react-router-dom": "^5.3.2",
    "axios": "^0.24.0",
    "firebase": "^9.4.1",
    "mobx": "^6.3.7",
    "mobx-react": "^7.2.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.0.2",
    "react-scripts": "4.0.3",
    "typescript": "^4.4.4",
    "web-vitals": "^1.1.2"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}